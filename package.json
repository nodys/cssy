  "version": "1.0.1",
  "author": "Jean Ponchon <jean.ponchon@novadiscovery.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/nodys/cssy.git"
  },
  "licenses": "MIT",
  "main": "./lib/cssy.js",
  "engines": {
    "node": ">= 0.10"
  },
  "scripts": {
    "test": "mocha",
    "spec": "mocha --reporter spec",
    "monitor": "mocha --reporter spec --watch",

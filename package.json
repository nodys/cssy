    "jshint": "./node_modules/.bin/jshint --exclude node_modules",
    "doc": "./support/updatedoc.js"
  },
  "dependencies": {
    "async": "^0.9.0",
    "chokidar": "^0.12.6",
    "concat-stream": "^1.4.6",
    "csswring": "^3.0.0",
    "debug": "^2.0.0",
    "extend": "^2.0.0",
    "lrio": "^1.1.0",
    "postcss": "^4.0.1",
    "relative-package": "^1.0.0",
    "resolve": "^1.0.0",
    "semver": "^4.2.0",
    "slash": "^1.0.0",
    "syntax-error": "^1.1.1",
    "through2": "^1.1.1"
  },
  "devDependencies": {
    "browserify": "^8.1.1",
    "docflux": "^1.1.2",
    "expect.js": "*",
    "jsdom": "^3.0.1",
    "jshint": "*",
    "less": "^2.2.0",
    "mocha": "*",
    "node-sass": "^1.2.3",
    "postcss-calc": "^3.0.0",
    "postcss-custom-properties": "^3.0.0",
    "rework": "^1.0.1",
    "rework-calc": "^1.1.0",
    "rework-vars": "^3.1.1",
    "stylus": "^0.49.1"

    "standard": "./node_modules/.bin/standard",
    "doc": "./support/updatedoc.js"
  },
  "dependencies": {
    "async": "^1.3.0",
    "autoprefixer": "^6.0.3",
    "chokidar": "^1.2.0",
    "concat-stream": "^1.5.0",
    "cssnano": "^3.2.0",
    "debug": "^2.0.0",
    "extend": "^3.0.0",
    "lrio": "^1.2.1",
    "postcss": "^5.0.8",
    "relative-package": "^1.0.0",
    "resolve": "^1.0.0",
    "semver": "^5.0.1",
    "syntax-error": "^1.1.1",
    "through2": "^2.0.0"
  },
  "devDependencies": {
    "browserify": "^11.2.0",
    "docflux": "^1.1.2",
    "expect.js": "*",
    "jsdom": "^6.5.1",
    "less": "^2.5.1",
    "mocha": "*",
    "node-sass": "^3.3.3",
    "postcss-calc": "^5.0.0",
    "postcss-custom-properties": "^5.0.0",
    "standard": "^5.3.1",
    "stylus": "^0.52.4"
  },
  "browser": "./src/cssy-browser.js",
